<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Project_1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Project_1">

    <ShellContent 
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}" />

    <FlyoutItem Title="Calculator">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:Calculator}" />
        </Tab>
    </FlyoutItem>

    <FlyoutItem Title="Progress">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:ProgressPage}" />
        </Tab>
    </FlyoutItem>

    <FlyoutItem Title="Sushi">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:Lab3}" />
        </Tab>
    </FlyoutItem>

</Shell>
